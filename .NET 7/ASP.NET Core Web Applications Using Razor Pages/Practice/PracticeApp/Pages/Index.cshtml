@page "{id?}"
@model IndexModel

@{ViewData["Title"] = "FL DMV Residency Check";}

<div class="container">
  <div class="row">
    <div class="text-center">
      <h1 class="display-4">@Model.PageTitle</h1>
    </div>
  </div>
  <div class="row">
    <form class="mt-0" method="get">
      <div class="row">
        @await Html.PartialAsync("_DisplayApplicationPartial", new List<ExemptionApplication>())
      </div>
    </form>
  </div>
</div>